{
  "name": "cy-viewer",
  "author": "Keiichiro Ono <kono@ucsd.edu>",
  "version": "v3.0.0",
  "description": "React component for graph data visualization",
  "license": "MIT",
  "keywords": [
    "cytoscape",
    "visualization",
    "graph",
    "bioinformatics"
  ],
  "main": "build/CyNetworkViewer.js",
  "files": [
    "webpack.config.js",
    ".preprocessor.js",
    ".travis.yml",
    "build/*",
    "src/*"
  ],
  "scripts": {
    "build": "./node_modules/.bin/webpack --progress --colors",
    "clean": "rm build/*",
    "lint": "eslint src/; exit 0",
    "test": "eslint src/ && jest",
    "coverage": "jest --coverage"
  },
  "homepage": "https://github.com/CyComponent/CyViewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/CyComponent/CyViewer.git"
  },
  "bugs": {
    "url": "https://github.com/CyComponent/CyViewer/issues"
  },
  "dependencies": {
    "cytoscape": "^2.7.11",
    "immutable": "^3.8.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "file-loader": "^0.11.2",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/.preprocessor.js"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "es6": true,
      "browser": true
    }
  }
}
